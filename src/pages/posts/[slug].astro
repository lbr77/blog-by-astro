---
import LayoutDefault from "../../layouts/LayoutDefault.astro";
import {getEntry} from 'astro:content';
import PostLayout from "../../layouts/LayoutPost.astro";
import Layout404 from "../../layouts/Layout404.astro";
const slug = Astro.params.slug;
const blog = await getEntry("posts", slug);
---
{blog && <LayoutDefault>
<PostLayout content={blog}/>
</LayoutDefault>}
{!blog && <LayoutDefault title={"找不到惹！"}> <Layout404/></LayoutDefault>}